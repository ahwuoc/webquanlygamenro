  point_post       Int              @default(0)
  last_post        Int              @default(0)
  role             Int              @default(-1)
  is_admin         Boolean          @default(false)
  last_time_login  DateTime         @default(now()) @db.Timestamp(0)
  last_time_logout DateTime         @default(now()) @db.Timestamp(0)
  last_time_off    DateTime         @default(now()) @db.Timestamp(0)
  ip_address       String?          @db.VarChar(50)
  active           Int              @default(0)
  mkc2             Int              @default(0)
  thoi_vang        Int              @default(0)
  server_login     Int              @default(-1)
  bd_player        Float?           @default(1)
  is_gift_box      Boolean?         @default(false)
  gift_time        String?          @default("0") @db.VarChar(255)
  reward           String?          @db.LongText
  tongnap          Int              @default(0)
  coin             Int              @default(0)
  vnd              Int?             @default(0)
  recaf            Int?
  gioithieu        Int              @default(0)
  mtvgt            Int              @default(0)
  vip1             Int              @default(0)
  vip2             Int              @default(0)
  vip3             Int              @default(0)
  vip4             Int              @default(0)
  vip5             Int              @default(0)
  vip6             Int              @default(0)
  admin            Int              @default(0)
  mabaove          String?          @db.VarChar(255)
  gmail            String?          @db.VarChar(100)
  tichdiem         Int              @default(0)
  xacminh          Int              @default(0)
  thoigian_xacminh Int              @default(0)
  del_pass2        DateTime?        @db.Timestamp(0)
  nrog8            Int              @default(0)
  mocnap           Int              @default(0)
  email            String?          @db.VarChar(255)
